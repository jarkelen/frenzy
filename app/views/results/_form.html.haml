= simple_form_for @result, :html => { :class => 'form-horizontal' } do |f|
  = f.input :club_home_id
  = f.input :club_away_id
  = f.input :score_home
  = f.input :score_away
  = f.input :gameround_id
  .form-actions
    = f.button :submit, :class => 'btn-primary'
    = link_to t('.cancel', :default => t("helpers.links.cancel")), results_path, :class => 'btn'
